{
  "manifest_version": 2,
  "name": "Show Keys",
  "version": "0.0.1",
  "description": "Show keys as you type.",
  "content_scripts":[
      {
        "matches":["<all_urls>"],
        "js":["index.js"]
      }
  ],
  "background": {
      "scripts": ["background.js"]
  },
  "browser_action": {
      "default_icon":"showkey.png"
  }
}